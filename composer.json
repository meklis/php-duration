{
    "name": "meklis/php-duration",
    "description": "Converts between colon formatted time, human-readable time and seconds",
    "keywords": [
        "duration",
        "time",
        "seconds",
        "convert"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Hill",
            "email": "kevinkhill@gmail.com",
            "role": "Creator"
        },
        {
            "name": "Duration Community",
            "homepage": "https://github.com/kevinkhill/php-duration/contributors"
        }
    ],
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.81",
        "phpunit/phpunit": "~4.8|~5.0",
        "satooshi/php-coveralls": "~1.0",
        "symfony/yaml": "~2.0",
        "symfony/config": "~2.0",
        "symfony/console": "~2.0",
        "symfony/stopwatch": "~2.0",
        "symfony/filesystem": "~2.0",
        "doctrine/instantiator": "1.0.4"
    },
    "autoload": {
        "psr-4": {
            "Khill\\Duration\\": "src/"
        }
    },
    "scripts": {
        "test": "phpunit -c phpunit.xml",
        "phpstan": "vendor/bin/phpstan"
    },
    "minimum-stability": "stable"
}
